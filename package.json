    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/node": "20.2.4",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "ai": "^2.2.31",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "concurrently": "^8.0.1",
    "eslint": "8.41.0",
    "eslint-config-next": "^14.1.0",
    "lucide-react": "^0.314.0",
    "next": "^14.1.0",
    "openai": "^4.26.0",
